{
    "name": "andrewfleming/zd-cli",
    "description": "A CLI wrapper around the Zendesk Support API.",
    "keywords": ["zendesk", "console", "cli"],
    "homepage": "https://laravel-zero.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/andrewfleming/zd-cli/issues",
        "source": "https://github.com/andrewfleming/zd-cli"
    },
    "authors": [
        {
            "name": "Andrew Fleming",
            "email": "aj@ajfleming.info"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "illuminate/encryption": "^8.10",
        "laminas/laminas-text": "^2.7",
        "laravel-zero/framework": "^8.0",
        "padraic/phar-updater": "^1.0.6",
        "stevenmaguire/oauth2-zendesk": "^2.1",
        "zendesk/zendesk_api_client_php": "^2.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/zd"]
}
